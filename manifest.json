{
	"manifest_version": 2,
	"name": "ISE Assistant",
	"version": "0.1",
	"options_page": "options.html",
    "permissions": [
    	"contextMenus",
    	"storage",
    	"https://*/"
	],
	"background": {
		"scripts": ["background.js"]
	},
	"browser_action": {
		"default_popup": "options.html",
		"default_title": "ISE Assistant"
	}
}