{
	"manifest_version": 2,
	"name": "ISE Assistant",
	"version": "0.3",
	"options_page": "options.html",
	"permissions": [
		"contextMenus",
		"storage",
		"notifications",
		"https://*/"
	],
	"background": {
		"scripts": ["background.js"]
	},
	"browser_action": {
		"default_popup": "options.html",
		"default_title": "ISE Assistant",
		"default_icon": {
			"16": "images/icon16.png",
			"48": "images/icon48.png"
		}
	},
	"icons": { 
		"16": "images/icon16.png",
		"48": "images/icon48.png",
		"128": "images/icon128.png" 
	}
}
